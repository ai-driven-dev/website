{
  "name": "weekly-ai-tips",
  "description": "A webapp that displays weekly posts for developers with AI tips and tricks.",
  "version": "0.1.0",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "doc": "typedoc --options typedoc.json --commentStyle block",
    "doc:watch": "nodemon",
    "doc:export": "curl -s https://raw.githubusercontent.com/alexsoyes/ai-driven-dev-community/main/scripts/project-documentation-typescript.sh | sh",
    "doc:structure": "curl -s https://raw.githubusercontent.com/alexsoyes/ai-driven-dev-community/main/scripts/project-structure.sh | EXCLUDE='documentation|.swc|run|.husky' sh",
    "dev": "source .env.local && next dev",
    "build": "next build",
    "start": "next start",
    "clean": "rm -rf tsconfig.tsbuildinfo node_modules package-lock.json .next && npm cache clean --force && npm install",
    "test": "jest --passWithNoTests --coverage --runInBand --detectOpenHandles",
    "test:watch": "jest --watch --watchAll",
    "test:coverage": "jest --coverage",
    "lint": "next lint && npx eslint . --ext .ts,.tsx && tsc --noEmit",
    "lint-fix": "npx eslint . --ext .ts,.tsx --fix",
    "release": "standard-version"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ],
    "*.{js,jsx,ts,tsx}": "npm test --bail --findRelatedTests"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@vercel/analytics": "^1.3.1",
    "@vercel/speed-insights": "^1.0.12",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "firebase": "^10.12.2",
    "firebase-admin": "^12.2.0",
    "lucide-react": "^0.401.0",
    "next": "^14.2.4",
    "next-firebase-auth-edge": "^1.5.3",
    "nuqs": "^1.17.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@testing-library/dom": "^10.3.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.1.4",
    "postcss": "^8.4.39",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.4.4",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typedoc": "^0.26.3",
    "typedoc-plugin-markdown": "^4.1.1",
    "typescript": "^5.5.3"
  }
}
